{
    "name": "tomatophp/tomato-browser-module",
    "type": "laravel-module",
    "description": "Browser to the files inside your app build for TomatoPHP",
    "keywords": [
        "php",
        "laravel",
        "browser",
        "file",
        "folders",
        "files",
        "upload"
    ],
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "EngFadyMondy@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/tomato-admin": "^1.2",
        "tomatophp/tomato-plugins": "^1.2",
        "tomatophp/console-helpers": "^1.1",
        "tomatophp/tomato-roles-module": "^1.0",
        "joshbrw/laravel-module-installer": "^2.0",
        "nwidart/laravel-modules": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [],
            "aliases": {

            }
        }
    },
    "autoload": {
        "psr-4": {
            "Modules\\TomatoBrowser\\": "",
            "Modules\\TomatoBrowser\\App\\": "app/",
            "Modules\\TomatoBrowser\\Database\\Factories\\": "database/factories/",
            "Modules\\TomatoBrowser\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\TomatoBrowser\\Tests\\": "tests/"
        }
    }
}
